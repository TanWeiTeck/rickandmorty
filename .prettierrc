{
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "printWidth": 150,
  "semi": true,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "importOrder": ["^react", "^@?\\w", "^[./]", "^../", "^@/", "^[../]", "^../../"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
